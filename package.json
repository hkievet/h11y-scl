{
  "name": "h11y-scl",
  "version": "0.2.4",
  "homepage": "http://hkievet.github.io/h11y-scl",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "pub": "svelte-kit sync && npm version patch && svelte-package boom && npm publish",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "sb-init": "storybook init -f",
    "clean": "rimraf .svelte-kit",
    "predeploy": "npm run build-storybook",
    "deploy-histoire": "npm run story:build && gh-pages -d .histoire/dist",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@histoire/plugin-svelte": "^0.16.3",
    "@playwright/test": "^1.32.3",
    "@storybook/addon-actions": "^7.0.0",
    "@storybook/addon-essentials": "^7.0.0",
    "@storybook/addon-interactions": "^7.0.0",
    "@storybook/addon-links": "^7.0.0",
    "@storybook/addon-svelte-csf": "^3.0.0",
    "@storybook/blocks": "^7.0.0-beta.50",
    "@storybook/builder-vite": "^7.0.0",
    "@storybook/cli": "^7.0.0",
    "@storybook/svelte": "^7.0.0",
    "@storybook/sveltekit": "^7.0.0",
    "@storybook/testing-library": "^0.1.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.15.7",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^5.0.0",
    "histoire": "^0.16.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.50",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "svelte-loader": "^3.1.7",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.1",
    "ts-dedent": "^2.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  },
  "peerDependencies": {
    "svelte": "^3.57.0"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "@cloudinary/url-gen": "^1.10.1",
    "postcss-nesting": "^12.0.0"
  }
}